{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "eslint:recommended",
    "google",
    "prettier"
  ],
  "globals": {
    "angular": "readonly",
    "app": "writable"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "max-len": "off",
    "camelcase": "off"
  },
  "plugins": [
    "jsdoc"
  ],
  "settings": {
    "jsdoc": {"mode": "typescript"}
  }
}
